<div class="container px-5 px-sm-0">
  <div class="row">
    
    <div class="col-md-8 offset-md-1">
      
      <h2>Users</h2>
      <therd>
        <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
        </tr>
      </therd>
      <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= image_tag @user.get_profile_image(80,80) %></td>
              <td><%= user.name %></td>
            </tr>
          <% end %>
        </tbody>
    </div>
  </div>
</div>